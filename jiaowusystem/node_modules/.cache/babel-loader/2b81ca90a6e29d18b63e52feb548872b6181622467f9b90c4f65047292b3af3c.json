   <router-view></router-view>\n      </el-container>\n    </div>\n  </div>\n  <!-- 对话框组件用于显示课程大纲 -->\n  <el-dialog v-model=\"text_dialogVisible\" title=\"课程大纲\" width=\"30%\" align-center center>\n    <div v-html=\"currentCourseText\"></div>\n    <template #footer>\n      <el-button @click=\"text_dialogVisible = false\">关闭</el-button>\n    </template>\n  </el-dialog>\n  <!-- 对话框组件用于显示审批意见 -->\n  <el-dialog v-model=\"note_dialogVisible\" title=\"课程大纲\" width=\"30%\" align-center center>\n    <div v-html=\"currentapfnote\"></div>\n    <template #footer>\n      <el-button @click=\"note_dialogVisible = false\">关闭</el-button>\n    </template>\n  </el-dialog>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      user_id: '',\n      user_account: '',\n      IsChangetchCourseRoute: false,\n      selectedRow: null, // 用来存储选中的行\n      text_dialogVisible: false, // 控制对话框的显示\n      currentCourseText: '', // 当前显示的课程大纲内容\n      note_dialogVisible: false,\n      currentapfnote: '',\n      tableData: [\n        { cla_id: 1, cou_name: '中级实训', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'可以可以' },\n        { cla_id: 2, cou_name: '高等数学', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'还行还行'  },\n        { cla_id: 3, cou_name: '大学物理', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'不中不中'  },\n        { cla_id: 4, cou_name: '数据结构与算法', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'可以可以'  },\n        { cla_id: 5, cou_name: '计算机组成原理', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'还行还行'  },\n        { cla_id: 6, cou_name: '编译原理', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'不中不中'  },\n        { cla_id: 7, cou_name: '数据库原理', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'可以可以'  },\n        { cla_id: 8, cou_name: '软件测试', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'还行还行'  },\n        { cla_id: 9, cou_name: '3D游戏', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'不中不中'  },\n        { cla_id: 10, cou_name: '数字图像处理', cou_status:'待审批', cou_ifexam:'是', cou_ifscore:'是', cou_term:3, cou_type:'专业课', cou_language:'中文', cou_rank:'专必', cou_school:'软件工程', cou_score:3, cou_text:'软件工程软件工程', apf_note:'可以可以'  },\n      ]\n    };\n  },\n  watch: {  \n    '$route'(to) {  \n      this.checkRoute(to); // 监听路由变化  \n    }  \n  }, \n  created() {\n    // 从查询参数中获取 id 和账号\n    this.user_id = this.$route.query.user_id;\n    this.user_account = this.$route.query.user_account;\n    this.checkRoute(this.$route); // 确认当前路由\n  },\n  methods: {\n    checkRoute(route) {  \n      // 检查当前路由是否是需要单独显示的页面  \n      const ChangetchCourseRoute = [  \n        'tch_editing_course',\n        'tch_apply_course' \n      ];  \n      this.IsChangetchCourseRoute = ChangetchCourseRoute.includes(route.name);  \n    },\n    goToTeacherHome() {\n      this.$router.go(-1);\n    },\n    goToEditingCourse() {\n      const { cla_id, cou_name } =