{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      change_info: {\n        user_id: \"\",\n        name: \"\",\n        // 默认姓名  \n        sex: \"\",\n        // 默认性别  \n        college: \"\",\n        tel: \"\",\n        user_account: \"\",\n        password: \"\",\n        new_password: \"\",\n        confirm_password: \"\"\n      },\n      user_id: '',\n      user_account: '',\n      rules: {\n        new_password: [{\n          required: true,\n          message: \"请输入新密码\",\n          trigger: 'blur'\n        }],\n        confirm_password: [{\n          required: true,\n          message: \"请确认密码\",\n          trigger: 'blur'\n        }, {\n          validator: this.validateConfirmPassword,\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.user_id = this.$route.query.user_id;\n    this.user_account = this.$route.query.user_account;\n    this.change_info.user_id = this.user_id;\n    this.change_info.user_account = this.user_account;\n  },\n  methods: {\n    validateConfirmPassword(rule, value, callback) {\n      if (value !== this.change_info.new_password) {\n        callback(new Error(\"两次输入的密码不一致\"));\n      } else {\n        callback();\n      }\n    },\n    goToTeacherHome() {\n      this.$router.go(-1);\n    },\n    saveInfo() {\n      console.log('信息已保存', this.change_info);\n    },\n    resetForm() {\n      this.change_info = {\n        user_id: \"\",\n        name: \"张三\",\n        // 重置姓名  \n        sex: \"男\",\n        // 重置性别  \n        college: \"\",\n        tel: \"\",\n        user_account: \"\",\n        password: \"\",\n        new_password: \"\",\n        confirm_password: \"\"\n      };\n    },\n    goToChangeInfo() {\n      this.$router.push({\n        name: 'tch_change_personal_info',\n        query: {\n          user_id: this.user_id,\n          user_account: this.user_account\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":