             <span>学院:</span>  \n              <span>{{ change_info.college }}</span>  \n            </div>  \n            <div class=\"info-item\">  \n              <span>电话:</span>  \n              <span>{{ change_info.tel }}</span>  \n            </div>  \n            <div class=\"info-item\">  \n              <span>账号:</span>  \n              <span>{{ change_info.user_account }}</span>  \n            </div>  \n            <div class=\"info-item\">  \n              <span>原密码:</span>  \n              <span>{{ change_info.password }}</span>  \n            </div>  \n            <el-form-item label=\"新密码:\" label-width=\"120px\" prop=\"new_password\">  \n              <el-input v-model=\"change_info.new_password\" type=\"password\"/>  \n            </el-form-item>  \n            <el-form-item label=\"确认密码:\" label-width=\"120px\" prop=\"confirm_password\">  \n              <el-input v-model=\"change_info.confirm_password\" type=\"password\"/>  \n            </el-form-item>  \n            <template #footer>  \n              <div class=\"button-container\">  \n                <el-button type=\"primary\" @click=\"saveInfo\">保存信息</el-button>  \n                <el-button @click=\"resetForm\">清空</el-button>  \n                <el-button type=\"default\" @click=\"goToChangeInfo\">修改个人信息</el-button>  \n              </div>  \n            </template>  \n          </el-card>  \n        </div>  \n      </el-main>  \n    </el-container>  \n  </div>  \n</template>  \n  \n<script>  \nexport default {  \n  data() {  \n    return {  \n      change_info: {  \n        user_id: \"\",  \n        name: \"\",  // 默认姓名  \n        sex: \"\",    // 默认性别  \n        college: \"\",  \n        tel: \"\",  \n        user_account: \"\",  \n        password: \"\",  \n        new_password: \"\",  \n        confirm_password: \"\"  \n      },  \n      user_id: '',  \n      user_account: '',  \n      rules: {  \n        new_password: [  \n          { required: true, message: \"请输入新密码\", trigger: 'blur' }  \n        ],  \n        confirm_password: [  \n          { required: true, message: \"请确认密码\", trigger: 'blur' },  \n          { validator: this.validateConfirmPassword, trigger: 'blur' }  \n        ]  \n      }  \n    };  \n  },  \n  created() {  \n    this.user_id = this.$route.query.user_id;  \n    this.user_account = this.$route.query.user_account;  \n    this.change_info.user_id = this.user_id;  \n    this.change_info.user_account = this.user_account;  \n  },  \n  methods: {  \n    validateConfirmPassword(rule, value, callback) {  \n      if (value !== this.change_info.new_password) {  \n        callback(new Error(\"两次输入的密码不一致\"));  \n      } else {  \n        callback();  \n      }  \n    },  \n    goToTeacherHome() {  \n      this.$router.go(-1);  \n    },  \n    saveInfo() {  \n      console.log('信息已保存', this.change_info);  \n    },  \n    resetForm() {  \n      this.change_info = {  \n        user_id: \"\",  \n        name: \"张三\", // 重置姓名  \n        sex: \"男\",   // 重置性别  \n        college: \"\",  \n        tel: \"\",  \n        user_account: \"\",  \n        password: \"\",  \n        new_password: \"\",  \n        confirm_password: \"\"  \n      };  \n    },  \n    goToChangeInfo() {  \n      this.$router.push({  \n        name: 'tch_change_personal_info',  \n        query: {  \n          user_id: this.user_id,  \n          user_account: this.user_account  \n        }  \n      });  \n    }  \n  }  \n};  \n</script>  \n  \n<style lang='stylus' scoped>  \n.change_personal_info {  \n  .el-container {  \n    display: flex;  \n    flex-direction: column;  \n    height: 100vh; // 设置容器高度为视口高度  \n  }  \n  .el-header {  \n    position: fixed;  \n    top: 0;  \n    left: 0;  \n    right: 0;  \n    background-color: var(--el-color-primary-light-3);  \n    color: black;  \n    height: 12%;  \n    z-index: 1000;  \n    display: flex;  \n    align-items: center;  \n    justify-content: space-between;  \n    padding: 0 20px;  \n  }  \n  .el-main {  \n    flex-grow: 1;  \n    margin-top: 2.5%;  \n    background-color: white;  \n    color: black;  \n    height: 80vh;  \n    overflow-y: auto;  \n  }  \n  .card-header {  \n    font-size: 28px;  \n    font-weight: bold;  \n  }  \n  .user-info {  \n    margin-left: 20px; /* 为用户信息添加左边距 */  \n    margin-right: 50px; /* 为用户信息添加右边距 */  \n  }  \n  .tch_personal_box-card {  \n    margin-left: 100px;  \n    width: 700px;  \n  }  \n  .info-item {  \n    display: flex;  \n    padding: 10px 0; /* 每行上下留出一些间距 */  \n    font-size: 28px;  \n  }  \n  .info-item span:first-child {  \n    font-weight: bold; /* 标签字体加粗 */  \n    min-width: 150px; /* 设置最小宽度以保证对齐 */   \n    margin-left: 200px;   \n    margin-right: 0px;  \n  }  \n  .badge {  \n    width: 85px;  \n    height: 85px;  \n    margin-left: 0;  \n    margin-right: 10px;  \n    border-radius: 50%;  \n  }  \n  .card-container {  \n    display: flex;  \n  }  \n  .avatar-container {  \n    text-align: center; /* 使标签文本居中对齐 */  \n    margin-left: 200px;  \n    margin-top: 100px;  \n  }  \n  .avatar-label {  \n    margin-top: 10px; /* 标签与头像之间的间距 */  \n    font-size: 25px; /* 标签字体大小 */  \n    font-weight: bold;  \n  }  \n  .avatar {  \n    width: 250px; /* 设置头像宽度 */  \n    height: 250px; /* 设置头像高度 */  \n    border-radius: 50%; /* 使头像为圆形 */  \n  }  \n  .button-container {  \n    display: flex;  \n    justify-content: space-between;  \n    margin-top: 20px;  \n  }  \n}  \n</style>  "],"mappings":";AAyEA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,IAAI,EAAE,EAAE;QAAG;QACXC,GAAG,EAAE,EAAE;QAAK;QACZC,OAAO,EAAE,EAAE;QACXC,GAAG,EAAE,EAAE;QACPC,YAAY,EAAE,EAAE;QAChBC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,gBAAgB,EAAE;MACpB,CAAC;MACDR,OAAO,EAAE,EAAE;MACXK,YAAY,EAAE,EAAE;MAChBI,KAAK,EAAE;QACLF,YAAY,EAAE,CACZ;UAAEG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAO,EACtD;QACDJ,gBAAgB,EAAE,CAChB;UAAEE,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAO,CAAC,EACrD;UAAEC,SAAS,EAAE,IAAI,CAACC,uBAAuB;UAAEF,OAAO,EAAE