{"ast":null,"code":"import { ref, onMounted, nextTick } from 'vue';\nimport { ElTable, ElTableColumn, ElButton, ElDialog } from 'element-plus';\nimport { useRouter } from 'vue-router';\nimport * as ECharts from \"echarts\";\nexport default {\n  __name: 'adm_course_score_interface',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    // eslint-disable-next-line\n    const router = useRouter();\n    const dialogVisible = ref(false);\n    const chartinstance = ref(null);\n    onMounted(() => {\n      studentScore.value = studentscore;\n    });\n    const studentScore = ref([]);\n    const studentscore = [{\n      number: \"1\",\n      tch: \"张三\",\n      Coursename: \"数学\",\n      year: \"2023\",\n      term: \"1\",\n      avg_score: \"90\"\n    }, {\n      number: \"2\",\n      tch: \"李四\",\n      Coursename: \"英语\",\n      year: \"2023\",\n      term: \"1\",\n      avg_score: \"85\"\n    }];\n    const handleEdit = (index, row) => {\n      console.log(index, row);\n      console.log(router);\n      dialogVisible.value = true; // 打开对话框\n    };\n    const goToTeacherHome = async () => {\n      router.go(-1);\n    };\n    const gatheringSourceOption = {\n      title: {\n        text: '成绩占比:',\n        left: 'left'\n      },\n      tooltip: {\n        trigger: 'item',\n        formatter: '{a} <br/>{b} : {c} ({d}%)'\n      },\n      legend: {\n        type: 'scroll',\n        orient: 'vertical',\n        right: 0,\n        top: 'center',\n        icon: 'circle',\n        selectedMode: 'multiple',\n        formatter: name => {\n          //legend显示数据格式化,每一个 legend 渲染都会回调这个函数\n          let data = gatheringSourceOption.series[0].data; //获取当前对象的series.data\n          let total = 0; //数据对象总和存放\n          let tarValue = 0; // 当前对象的value 值\n          for (let i = 0, l = data.length; i < l; i++) {\n            //循环饼状图数据对象\n            total += data[i].value; //计算出当前对象总和\n            if (data[i].name == name) {\n              tarValue = data[i].value; //存放当前渲染的legend 对象\n            }\n          }\n          let p = (tarValue / total * 100).toFixed(2); //百分比保留小数点后两位\n          return `${name}  |  ${p}%  `;\n        }\n      },\n      series: [{\n        name: '最近7日占比',\n        type: 'pie',\n        radius: '70%',\n        center: ['25%', '55%'],\n        selectedMode: 'single',\n        data: [],\n        label: {\n          show: false\n        },\n        itemStyle: {\n          emphasis: {\n            shadowBlur: 10,\n            shadowOffsetX: 0,\n            shadowColor: 'rgba(0, 0, 0, 0.5)'\n          }\n        }\n      }]\n    };\n    const eChartsInit = async (domId, theme, opt) => {\n      chartinstance.value = ECharts.init(document.getElementById(domId), theme);\n      chartinstance.value.setOption(opt);\n      window.addEventListener('resize', () => {\n        chartinstance.value.resize();\n      });\n    };\n    const getProportionData = async () => {\n      var sourceList = [{\n        value: 1,\n        name: '苹果'\n      }, {\n        value: 2,\n        name: '草莓'\n      }, {\n        value: 2,\n        name: '西瓜'\n      }, {\n        value: 3,\n        name: '桔子'\n      }, {\n        value: 2,\n        name: '葡萄'\n      }, {\n        value: 4,\n        name: '柿子'\n      }];\n      gatheringSourceOption.series[0].data = sourceList;\n      eChartsInit('home_gathering_source', 'light', gatheringSourceOption);\n    };\n    const open = async () => {\n      nextTick(() => {\n        getProportionData();\n      });\n    };\n    const destorychart = async () => {\n      chartinstance.value.dispose();\n      chartinstance.value = null;\n    };\n    const yearoptions = [{\n      value: '2022',\n      label: '2022'\n    }, {\n      value: '2023',\n      label: '2023'\n    }];\n    const termoptions = [{\n      value: '1',\n      label: '第一学期'\n    }, {\n      value: '2',\n      label: '第二学期'\n    }];\n    const collegeoptions = [{\n      value: '1',\n      label: '计算机学院'\n    }, {\n      value: '2',\n      label: '外国语学院'\n    }, {\n      value: '3',\n      label: '经济学院'\n    }, {\n      value: '4',\n      label: '管理学院'\n    }];\n    const __returned__ = {\n      router,\n      dialogVisible,\n      chartinstance,\n      studentScore,\n      studentscore,\n      handleEdit,\n      goToTeacherHome,\n      gatheringSourceOption,\n      eChartsInit,\n      getProportionData,\n      open,\n      destorychart,\n      yearoptions,\n      termoptions,\n      collegeoptions,\n      ref,\n      onMounted,\n      nextTick,\n      get ElTable() {\n        return ElTable;\n      },\n      get ElTableColumn() {\n        return ElTableColumn;\n      },\n      get ElButton() {\n        return ElButton;\n      },\n      get ElDialog() {\n        return ElDialog;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      get ECharts() {\n        return ECharts;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","nextTick","ElTable","ElTableColumn","ElButton","ElDialog","useRouter","ECharts","router","dialogVisible","chartinstance","studentScore","value","studentscore","number","tch","Coursename","year","term","avg_score","handleEdit","index","row","console","log","goToTeacherHome","go","gatheringSourceOption","title","text","left","tooltip","trigger","formatter","legend","type","orient","right","top","icon","selectedMode","name","data","series","total","tarValue","i","l","length","p","toFixed","radius","center","label","show","itemStyle","emphasis","shadowBlur","shadowOffsetX","shadowColor","eChartsInit","domId","theme","opt","init","document","getElementById","setOption","window","addEventListener","resize","getProportionData","sourceList","open","destorychart","dispose","yearoptions","termoptions","collegeoptions"],"sources":["D:/vue_project/test_project/jiaowusystem2/src/views/main/adm_course_score_interface.vue"],"sourcesContent":["<!--教学班成绩（教务）-->\r\n<template>\r\n  <div>\r\n    <el-container>\r\n        <el-header> \r\n          <img src=\"@/assets/2.jpg\" alt=\"Icon\" class=\"badge\"> \r\n          <h3 style=\"margin-right: auto;\">某某学校教务员端——教学班成绩页面</h3>    \r\n          <div class=\"user-info\">  \r\n            <h3>账号：{{ user_account }}</h3>  \r\n            <h3>ID: {{ user_id }}</h3>  \r\n          </div>  \r\n          <el-button type=\"danger\" style=\"float: right;\" @click=\"goToTeacherHome\">返回</el-button>  \r\n        </el-header>\r\n      <el-main>\r\n        <div>\r\n          <el-row>\r\n            <el-col :span=\"24\">\r\n              <div class=\"text-left\">成绩查询</div>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row class=\"container\">\r\n            <el-col :span=\"24\">\r\n              <td class=\"line\">\r\n                <div />\r\n              </td>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row gutter=\"20\" :justify=\"center\" class=\"container\">\r\n            <!-- <el-col :span=\"4\"> 学年： </el-col> -->\r\n            <el-col :span=\"5\" :offset=\"2\">\r\n              <!-- 选择学年 -->\r\n              <div style=\"display: flex; align-items: center;\">\r\n                <span style=\"margin-right: 6px;\">学年：</span>\r\n                <el-select v-model=\"year\" placeholder=\"Select\" style=\"width: 240px\">\r\n                  <el-option v-for=\"item in yearoptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" />\r\n                </el-select>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"5\">\r\n              <!-- 选择学期 -->\r\n              <div style=\"display: flex; align-items: center;\">\r\n                <span style=\"margin-right: 6px;\">学期：</span>\r\n                <el-select v-model=\"term\" placeholder=\"Select\" style=\"width: 240px\">\r\n                  <el-option v-for=\"item in termoptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" />\r\n                </el-select>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"5\">\r\n              <!-- 选择学院 -->\r\n              <div style=\"display: flex; align-items: center;\">\r\n                <span style=\"margin-right: 6px;\">学院：</span>\r\n                <el-select v-model=\"college\" placeholder=\"Select\" style=\"width: 240px\">\r\n                  <el-option v-for=\"item in collegeoptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" />\r\n                </el-select>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"4\" :offset=\"2\">\r\n              <!-- 查询按钮 -->\r\n              <el-button :dark=\"isDark\" @click=\"fetchStudentScore\">查询</el-button>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row class=\"container\" justify=\"center\">\r\n            <el-col :span=\"23\">\r\n              <el-table :data=\"studentScore\" style=\"width: 100%\" :header-cell-style=\"{ backgroundColor: 'var(--el-color-primary-light-3)', color: '#ffffff' }\">\r\n                <el-table-column prop=\"number\" label=\"教学班编号\" />\r\n                <el-table-column prop=\"tch\" label=\"教师\" />\r\n                <el-table-column prop=\"Coursename\" label=\"课程名\" />\r\n                <el-table-column prop=\"year\" label=\"学年\" />\r\n                <el-table-column prop=\"term\" label=\"学期\" />\r\n                <el-table-column prop=\"avg_score\" label=\"平均成绩\" />\r\n                <el-table-column prop=\"operation\" label=\"操作\">\r\n                  <template #default=\"scope\">\r\n                    <el-button size=\"small\" @click=\"handleEdit(scope.$index, scope.row)\">\r\n                      详情\r\n                    </el-button>\r\n                  </template>\r\n                </el-table-column>\r\n              </el-table>\r\n            </el-col>\r\n          </el-row>\r\n          <el-dialog v-model=\"dialogVisible\" title=\"成绩详情\" width=\"1000\" @open=\"open()\" @closed=\"destorychart()\">\r\n            <div class=\"detail\">\r\n              <div class=\"left\">\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>教学班编号</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>班主任</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>专业</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>课程名</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>平均分</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>最高分</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>最低分</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>极值</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n                <el-row class=\"container\">\r\n                  <el-col>\r\n                    <el-text>及格率</el-text>\r\n                  </el-col>\r\n                </el-row>\r\n              </div>\r\n              <div class=\"right\">\r\n                <!--饼状图and环形图-->\r\n                <div class=\"graph-pie-warp\">\r\n                  <el-row>\r\n    