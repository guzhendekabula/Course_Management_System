prop=\"cou_ifexam\">\r\n                <el-radio-group v-model=\"applying_course.cou_ifexam\">\r\n                  <el-radio :label=\"true\">是</el-radio>\r\n                  <el-radio :label=\"false\">否</el-radio>\r\n                </el-radio-group>\r\n              </el-form-item>\r\n              <el-form-item label=\"是否评分\" prop=\"cou_ifscore\">\r\n                <el-radio-group v-model=\"applying_course.cou_ifscore\">\r\n                  <el-radio :label=\"true\">是</el-radio>\r\n                  <el-radio :label=\"false\">否</el-radio>\r\n                </el-radio-group>\r\n              </el-form-item>\r\n              <el-form-item label=\"开课学期\" prop=\"cou_term\">\r\n                <el-input v-model=\"applying_course.cou_term\" placeholder=\"请输入开课学期\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"课程类型\" prop=\"cou_type\">\r\n                <el-input v-model=\"applying_course.cou_type\" placeholder=\"请输入课程类型\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"课程语言\" prop=\"cou_language\">\r\n                <el-input v-model=\"applying_course.cou_language\" placeholder=\"请输入课程语言\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"课程等级\" prop=\"cou_rank\">\r\n                <el-input v-model=\"applying_course.cou_rank\" placeholder=\"请输入课程等级\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"课程状态\" prop=\"cou_status\">\r\n                <el-input v-model=\"applying_course.cou_status\" placeholder=\"请输入课程状态\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"授课单位\" prop=\"cou_school\">\r\n                <el-input v-model=\"applying_course.cou_school\" placeholder=\"请输入授课单位\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"课程大纲\" prop=\"cou_text\">\r\n                <el-input type=\"textarea\" v-model=\"applying_course.cou_text\" placeholder=\"请输入课程大纲\"></el-input>\r\n              </el-form-item>\r\n            </el-form>\r\n              <template #footer>  \r\n                <div class=\"foot_button\">\r\n                  <el-button type=\"primary\" size=\"large\" style=\"width: 200px; height: 50px; font-size: 18px;\" @click=\"onSubmit\">申请开课</el-button>\r\n                  <el-button size=\"large\" style=\"width: 200px; height: 50px; font-size: 18px;\" @click=\"resetForm\">清空</el-button>\r\n                </div> \r\n              </template>    \r\n          </el-card>\r\n        </el-main>\r\n      </el-container>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  \r\n  export default {\r\n    data() {  \r\n      return {    \r\n        user_id: \"\",  \r\n        user_account: \"\",\r\n        applying_course: {\r\n          cou_name: \"\",\r\n          cou_ifexam: true,\r\n          cou_ifscore: true,\r\n          cou_term: \"\",\r\n          cou_type: \"\",\r\n          cou_language: \"\",\r\n          cou_rank: \"\",\r\n          cou_status: \"\",\r\n          cou_school: \"\",\r\n          cou_text: \"\" // 修改字段名为 cou_text\r\n        },\r\n        rules: {\r\n          cou_name: [{ required: true, message: '请输入课程名', trigger: 'blur' }],\r\n          cou_ifexam: [{ required: true, message: '请选择是否考试', trigger: 'change' }],\r\n          cou_ifscore: [{ required: true, message: '请选择是否评分', trigger: 'change' }],\r\n          cou_term: [{ required: true, message: '请输入开课学期', trigger: 'blur' }],\r\n          cou_type: [{ required: true, message: '请输入课程类型', trigger: 'blur' }],\r\n          cou_language: [{ required: true, message: '请输入课程语言', trigger: 'blur' }],\r\n          cou_rank: [{ required: true, message: '请输入课程等级', trigger: 'blur' }],\r\n          cou_status: [{ required: true, message: '请输入课程状态', trigger: 'blur' }],\r\n          cou_school: [{ required: true, message: '请输入授课单位', trigger: 'blur' }],\r\n          cou_text: [{ required: true, message: '请输入课程大纲', trigger: 'blur' }] // 修改字段名为 cou_text\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n      // 从查询参数中获取 id 和账号\r\n      this.user_id = this.$route.query.user_id;\r\n      this.user_account = this.$route.query.user_account;\r\n    },\r\n    methods: {\r\n      goToTeacherApproval() {\r\n        this.$router.go(-1); // 返回上一页\r\n      },\r\n      onSubmit() {\r\n        this.$refs.applying_course.validate((valid) => {\r\n          if (valid) {\r\n            // 处理申请开课逻辑\r\n            console.log('申请开课成功', this.applying_course);\r\n          } else {\r\n            console.log('表单验证失败');\r\n            return false;\r\n          }\r\n        });\r\n      },\r\n      resetForm() {\r\n        this.$refs.applying_course.resetFields();\r\n      }\r\n    },\r\n  }\r\n  \r\n  </script>\r\n  \r\n  <style lang='stylus' scoped>\r\n  html, body {  \r\n    height: 100%; /* 设置html和body的高度为100% */  \r\n    margin: 0; /* 去掉默认的margin */  \r\n    overflow: hidden; /* 隐藏body的滚动条 */  \r\n  }  \r\n  \r\n  .el-container {  \r\n    display: flex;  \r\n    flex-direction: column;  \r\n    height: 100%; /* 设置容器高度为100% */  \r\n  }  \r\n  \r\n  .el-header {  \r\n    position: fixed;  \r\n    top: 0;  \r\n    left: 0;  \r\n    right: 0;  \r\n    background-color: var(--el-color-primary-light-3);  \r\n    color: black;  \r\n    height: 12%;   \r\n    z-index: 1000;  \r\n    display: flex;  \r\n    align-items: center;  \r\n    justify-content: space-between;  \r\n    padding: 0 20px;  \r\n  }  \r\n  .apply_course {  \r\n    height: 100%;  \r\n    overflow-y: auto; /* 允许垂直滚动 */  \r\n  } \r\n  .badge {  \r\n    width: 85px;  \r\n    height: 85px;  \r\n    margin-left: 0;  \r\n    margin-right: 10px;  \r\n    border-radius: 50%;  \r\n  }  \r\n  .el-card {  \r\n    margin-left: 250px;  \r\n    margin-top: 50px;  \r\n  }  \r\n  .user-info {  \r\n    margin-left: 20px;   \r\n    margin-right: 50px;   \r\n  }  \r\n  .tch_apply_course_box-card {  \r\n    margin-left: 350px;  \r\n    width: 700px;  \r\n  }  \r\n  .foot_button{\r\n    text-align: center; \r\n    gap: 100px\r\n  }\r\n  \r\n  </style>"],"mappings":"AAwEE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,EAAE;MAChBC,eAAe,EAAE;QACfC,QAAQ,EAAE,EAAE;QACZC,UAAU,EAAE,IAAI;QAChBC,WAAW,EAAE,IAAI;QACjBC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,QAAQ,EAAE,EAAE;QACZC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE,EAAE;QACdC,QAAQ,EAAE,EAAC,CAAE;MACf,CAAC;MACDC,KAAK,EAAE;QACLV,QAAQ,EAAE,CAAC;UAAEW,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QAClEZ,UAAU,EAAE,CAAC;UAAEU,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAS,CAAC,CAAC;QACvEX,WAAW,EAAE,CAAC;UAAES,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAS,CAAC,CAAC;QACxEV,QAAQ,EAAE,CAAC;UAAEQ,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QACnET,QAAQ,EAAE,CAAC;UAAEO,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QACnER,YAAY,EAAE,CAAC;UAAEM,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QACvEP,QAAQ,EAAE,CAAC;UAAEK,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QACnEN,UAAU,EAAE,CAAC;UAAEI,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QACrEL,UAAU,EAAE,CAAC;UAAEG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,CAAC;QACrEJ,QAAQ,EAAE,CAAC;UAAEE,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,EAAE;MACtE;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACjB,OAAM,GAAI,IAAI,CAACkB,MAAM,CAACC,KAAK,CAACnB,OAAO;IACxC,IAAI,CAACC,YAAW,GAAI,IAAI,CAACiB,MAAM,CAACC,KAAK,CAAClB,YAAY;EACpD,CAAC;EACDmB,OAAO,EAAE;IACPC,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAACC,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE;IACvB,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,IAAI,CAACC,KAAK,CAACvB,eAAe,CAACwB,QAAQ,CAAEC,KAAK,IAAK;QAC7C,IAAIA,KAAK,EAAE;UACT;UACAC,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAE,IAAI,CAAC3B,eAAe,CAAC;QAC7C,OAAO;UACL0B,OAAO,CAACC,GAAG,CAAC,QAAQ,CAAC;UACrB,OAAO,KAAK;QACd;MACF,CAAC,CAAC;IACJ,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACL,KAAK,CAACv