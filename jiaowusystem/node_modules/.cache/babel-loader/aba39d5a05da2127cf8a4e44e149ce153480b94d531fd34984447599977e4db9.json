ces":["D:\\vue_project\\test_project\\jiaowusystem\\src\\views\\main\\tch_change_personal_info.vue"],"sourcesContent":["<template>  \r\n    <div class=\"change_personal_info\">  \r\n      <el-container>  \r\n        <el-header>   \r\n          <img src=\"@/assets/2.jpg\" alt=\"Icon\" class=\"badge\">   \r\n          <h3 style=\"margin-right: auto;\">某某学校教师端——修改个人信息页面</h3>    \r\n          <div class=\"user-info\">  \r\n            <h3>账号：{{ change_info.user_account }}</h3>  \r\n            <h3>ID: {{ change_info.user_id }}</h3>  \r\n          </div>  \r\n          <el-button type=\"danger\" style=\"float: right;\" @click=\"goToTeacherHome\">返回</el-button>  \r\n        </el-header>  \r\n        <el-main>  \r\n          <el-card style=\"max-width: 800px\" class=\"tch_personal_box-card\">  \r\n            <template #header>  \r\n              <div class=\"card-header\">  \r\n                <span>个人信息</span>  \r\n              </div>  \r\n            </template>  \r\n            <div class=\"info\">\r\n            <el-form ref=\"change_info\" :model=\"change_info\" :rules=\"rules\" status-icon>  \r\n              <el-form-item label=\"教师ID:\" label-width=\"120px\">  \r\n                <span>{{ change_info.user_id }}</span> <!-- 显示教师ID -->  \r\n              </el-form-item>  \r\n              <el-form-item label=\"姓名:\" label-width=\"120px\">  \r\n                <span>{{ change_info.name }}</span> <!-- 显示姓名 -->  \r\n              </el-form-item>  \r\n              <el-form-item label=\"性别:\" label-width=\"120px\">  \r\n                <span>{{ change_info.sex }}</span> <!-- 显示性别 -->  \r\n              </el-form-item>  \r\n              <el-form-item label=\"学院:\" label-width=\"120px\" prop=\"college\">  \r\n                <el-input v-model=\"change_info.college\" type=\"text\"/>  \r\n              </el-form-item>  \r\n              <el-form-item label=\"电话:\" label-width=\"120px\" prop=\"tel\">  \r\n                <el-input v-model=\"change_info.tel\" type=\"text\"/>  \r\n              </el-form-item>  \r\n              <el-form-item label=\"账号:\" label-width=\"120px\">  \r\n                <span>{{ change_info.user_account }}</span> <!-- 显示账号 -->  \r\n              </el-form-item>  \r\n              <el-form-item label=\"原密码:\" label-width=\"120px\">  \r\n                <span>{{ change_info.password }}</span> <!-- 显示原密码 -->  \r\n              </el-form-item>  \r\n              <el-form-item label=\"新密码:\" label-width=\"120px\" prop=\"new_password\">  \r\n                <el-input v-model=\"change_info.new_password\" type=\"password\"/>  \r\n              </el-form-item>  \r\n              <el-form-item label=\"确认密码:\" label-width=\"120px\" prop=\"confirm_password\">  \r\n                <el-input v-model=\"change_info.confirm_password\" type=\"password\"/>  \r\n              </el-form-item>\r\n            </el-form> \r\n        </div>\r\n              <template #footer>\r\n                <div class=\"button-container\">  \r\n                  <el-button type=\"primary\" @click=\"saveInfo\" style=\"margin-right: 100px;\">保存信息</el-button>  \r\n                  <el-button @click=\"resetForm('change_info')\">清空</el-button>  \r\n                </div>\r\n              </template> \r\n          </el-card>  \r\n        </el-main>  \r\n      </el-container>  \r\n    </div>  \r\n  </template>  \r\n    \r\n  <script>  \r\n  export default {  \r\n    data() {  \r\n      return {  \r\n        change_info: {  \r\n          user_id: \"\",  \r\n          name: \"\",  // 学生姓名示例  \r\n          sex: \"\",    // 学生性别示例  \r\n          college: \"\",  \r\n          tel: \"\",  \r\n          user_account: \"\",  \r\n          password: \"\",  \r\n          new_password: \"\",  \r\n          confirm_password: \"\"  \r\n        },  \r\n  \r\n        rules: {  \r\n          college: [  \r\n            { required: true, message: \"请输入学院\", trigger: 'blur' }  \r\n          ],  \r\n          tel: [  \r\n            { required: true, message: \"请输入电话\", trigger: 'blur' }  \r\n          ],  \r\n          new_password: [  \r\n            { required: true, message: \"请输入新密码\", trigger: 'blur' }  \r\n          ],  \r\n          confirm_password: [  \r\n            { required: true, message: \"请确认密码\", trigger: 'blur' },  \r\n            { validator: this.validateConfirmPassword, trigger: 'blur' }  \r\n          ]  \r\n        }  \r\n      };  \r\n    },  \r\n    created() {  \r\n \r\n      this.change_info.user_id = this.$route.query.user_id;  \r\n      this.change_info.user_account = this.$route.query.user_account;  \r\n    },  \r\n    methods: {  \r\n      validateConfirmPassword(rule, value, callback) {  \r\n        if (value !== this.change_info.new_password) {  \r\n          callback(new Error(\"两次输入的密码不一致\"));  \r\n        } else {  \r\n          callback();  \r\n        }  \r\n      },  \r\n      goToTeacherHome() {  \r\n        this.$router.go(-1);  \r\n      },  \r\n      saveInfo() {  \r\n        // 添加保存信息的逻辑  \r\n        console.log('信息已保存', this.change_info);  \r\n      },  \r\n      resetForm(formName) {  \r\n        this.$refs[formName].resetFields();\r\n      }  \r\n    }  \r\n  };  \r\n  </script>  \r\n    \r\n  <style lang='stylus' scoped>  \r\n  .change_personal_info {  \r\n    .el-container {  \r\n      display: flex;  \r\n      flex-direction: column;  \r\n      height: 100vh; // 设置容器高度为视口高度  \r\n    }  \r\n    .el-header {  \r\n      position: fixed;  \r\n      top: 0;  \r\n      left: 0;  \r\n      right: 0;  \r\n      background-color: var(--el-color-primary-light-3);  \r\n      color: black;  \r\n      height: 12%;  \r\n      z-index: 1000;  \r\n      display: flex;  \r\n      align-items: center;  \r\n      justify-content: space-between;  \r\n      padding: 0 20px;  \r\n    }  \r\n    .user-info {  \r\n      margin-left: 20px; /* 为用户信息添加左边距 */  \r\n      margin-right: 50px; /* 为用户信息添加右边距 */  \r\n    }  \r\n    .badge {  \r\n      width: 85px;  \r\n      height: 85px;  \r\n      margin-left: 0;  \r\n      margin-right: 10px;  \r\n      border-radius: 50%;  \r\n    }  \r\n    .el-card{\r\n        margin-left: 250px;\r\n        margin-top: 50px;\r\n    }\r\n    .button-container{\r\n        margin-left: 0px; /* 调整为你希望的距离 */\r\n        text-align: center;\r\n    }\r\n  }  \r\n  </style>"],"mappings":"AA+DE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,IAAI,EAAE,EAAE;QAAG;QACXC,GAAG,EAAE,EAAE;QAAK;QACZC,OAAO,EAAE,EAAE;QACXC,GAAG,EAAE,EAAE;QACPC,YAAY,EAAE,EAAE;QAChBC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,gBAAgB,EAAE;MACpB,CAAC;MAEDC,KAAK,EAAE;QACLN,OAAO,EAAE,CACP;UAAEO,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAO,EACrD;QACDR,GAAG,EAAE,CACH;UAAEM,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAO,EACrD;QACDL,YAAY,EAAE,CACZ;UAAEG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAO,EACtD;QACDJ,gBAAgB,EAAE,CAChB;UAAEE,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAO,CAAC,EACrD;UAAEC,SAAS,EAAE,IAAI,CAACC,uBAAuB;UAAEF,OAAO,EAAE;QAAO;MAE/D;IACF,CAAC;EACH,CAAC;EACDG,OAAOA,CAAA,EAAG;IAER,IAAI,CAAChB,WAAW,CAACC,OAAM,GAAI,IAAI,CAACgB,MAAM,CAACC,KAAK,CAACjB,OAAO;IACpD,IAAI,CAACD,WAAW,CAACM,YAAW,GAAI,IAAI,CAACW,MAAM,CAACC,KAAK,CAACZ,YAAY;EAChE,CAAC;EACDa,OAAO,EAAE;IACPJ,uBAAuBA,CAACK,IAAI,EAAEC,KAAK,EAAEC,QAAQ,EAAE;MAC7C,IAAID,KAAI,KAAM,IAAI,CAACrB,WAAW,CAACQ,YAAY,EAAE;QAC3Cc,QAAQ,CAAC,IAAIC,KAAK,CAAC,YAAY,CAAC,CAAC;MACnC,OAAO;QACLD,QAAQ,CAAC,CAAC;MACZ;IACF,CAAC;IACDE,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,CAAC;IACrB,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT;MACAC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAE,IAAI,CAAC7B,WAAW,CAAC;IACxC,CAAC;IACD8B,SAASA,CAACC,QAAQ,EAAE;MAClB,IAAI,CAACC,KAAK,CAACD,QAAQ,CAAC,CAACE,WAAW,CAAC,CAAC;IACpC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}