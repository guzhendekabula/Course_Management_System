e\"> \r\n          <h3 style=\"margin-right: auto;\">某某学校教师端——我的评分页面</h3>    \r\n          <div class=\"user-info\">  \r\n            <h3>账号：{{ user_account }}</h3>  \r\n            <h3>ID: {{ user_id }}</h3>  \r\n          </div>  \r\n          <el-button type=\"danger\" style=\"float: right;\" @click=\"goToTeacherGrade\">返回</el-button>  \r\n        </el-header>\r\n        <el-main>\r\n          <div class=\"lab\">\r\n           <h3>教学班号：{{ cla_id }}</h3>\r\n           <h3>课程名：{{ cou_name }}</h3> \r\n           <h3>班级人数：{{ member_count }}</h3>\r\n          </div>\r\n          <!--设计一个可以打分的表格，表格有学号stu_id、姓名stu_name、评分项1sco_item_1、评分项2sco_item_2、评分项3sco_item_3、评分项4sco_item_4、备注sco_note、总分sco_socore_1，根据评分项1，2，3，4可以计算出总分-->\r\n          <!-- 打分表格 -->\r\n        <el-table :data=\"students\" style=\"width: 100%\" border height=\"350px\">\r\n          <el-table-column label=\"学号\" prop=\"stu_id\" width=\"100px\"></el-table-column>\r\n          <el-table-column label=\"姓名\" prop=\"stu_name\" width=\"150px\"></el-table-column>\r\n          \r\n          <!-- 评分项列-->\r\n          <el-table-column label=\"评分项1\" prop=\"sco_item_1\">\r\n            <template v-slot=\"scope\">\r\n              <el-input-number v-model=\"scope.row.sco_item_1\" :min=\"0\" :max=\"100\" label=\"评分项1\"></el-input-number>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <el-table-column label=\"评分项2\" prop=\"sco_item_2\">\r\n            <template v-slot=\"scope\">\r\n              <el-input-number v-model=\"scope.row.sco_item_2\" :min=\"0\" :max=\"100\" label=\"评分项2\"></el-input-number>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <el-table-column label=\"评分项3\" prop=\"sco_item_3\">\r\n            <template v-slot=\"scope\">\r\n              <el-input-number v-model=\"scope.row.sco_item_3\" :min=\"0\" :max=\"100\" label=\"评分项3\"></el-input-number>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <el-table-column label=\"评分项4\" prop=\"sco_item_4\">\r\n            <template v-slot=\"scope\">\r\n              <el-input-number v-model=\"scope.row.sco_item_4\" :min=\"0\" :max=\"100\" label=\"评分项4\"></el-input-number>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 备注列 -->\r\n          <el-table-column label=\"备注\" prop=\"sco_note\">\r\n            <template v-slot=\"scope\">\r\n                <el-button @click=\"openRemarkDialog(scope.row)\" type=\"text\">编辑</el-button>\r\n                <span>{{ scope.row.sco_note }}</span>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 总分列 -->\r\n          <el-table-column label=\"总分\" prop=\"sco_score\">\r\n            <template v-slot=\"scope\">\r\n              <span>{{ calculateTotalScore(scope.row) }}</span>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        </el-main>\r\n        <el-dialog\r\n  title=\"编辑备注\"\r\n  v-model:visible=\"dialogVisible\"\r\n  width=\"400px\"\r\n  @close=\"resetRemark\">\r\n  <el-input\r\n    type=\"textarea\"\r\n    v-model=\"currentRemark\"\r\n    placeholder=\"请输入备注内容\"\r\n    rows=\"4\">\r\n  </el-input>\r\n\r\n  <!-- 使用 v-slot 语法来绑定具名插槽 -->\r\n  <template v-slot:footer>\r\n    <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n    <el-button type=\"primary\" @click=\"saveRemark\">保存</el-button>\r\n  </template>\r\n</el-dialog>\r\n    </el-container>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {  \r\n    return {    \r\n      user_id: \"\",  \r\n      user_account: \"\",\r\n      cla_id: 0,\r\n      cou_name: \"\",\r\n      member_count: 0,\r\n      dialogVisible: false, // 控制对话框的显示与隐藏\r\n      currentRemark: '', // 当前编辑的备注\r\n      selectedRow: null, // 当前选中的行\r\n      students: [\r\n        { stu_id: 'S001', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S002', stu_name: '李四', sco_item_1: 70, sco_item_2: 75, sco_item_3: 80, sco_item_4: 78, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S003', stu_name: '王五', sco_item_1: 85, sco_item_2: 88, sco_item_3: 92, sco_item_4: 90, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S004', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S005', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S006', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S007', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S008', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S009', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        { stu_id: 'S010', stu_name: '张三', sco_item_1: 80, sco_item_2: 85, sco_item_3: 90, sco_item_4: 88, sco_note: '', sco_score: 0 },\r\n        // 更多学生...\r\n      ],\r\n    }\r\n  },\r\n  created() {\r\n    // 从查询参数中获取 id 和账号\r\n    this.user_id = this.$route.query.user_id;\r\n    this.user_account = this.$route.query.user_account;\r\n    this.cla_id = this.$route.query.cla_id;\r\n    this.cou_name = this.$route.query.cou_name;\r\n  },\r\n  methods: {\r\n    openRemarkDialog(row) {\r\n      this.selectedRow = row;\r\n      this.currentRemark = row.sco_note;\r\n      this.dialogVisible = true; // 打开对话框\r\n    },\r\n    saveRemark() {\r\n      this.selectedRow.sco_note = this.currentRemark; // 保存备注\r\n      this.dialogVisible = false; // 关闭对话框\r\n    },\r\n    resetRemark() {\r\n      this.currentRemark = '';\r\n    },\r\n    goToTeacherGrade() {\r\n      this.$router.go(-1); // 返回上一页\r\n    },\r\n    // 计算每个学生的总分\r\n    calculateTotalScore(student) {\r\n      return (student.sco_item_1 + student.sco_item_2 + student.sco_item_3 + student.sco_item_4)/4;\r\n    }\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style lang='stylus' scoped>\r\nhtml, body {  \r\n  height: 100%; /* 设置html和body的高度为100% */  \r\n  margin: 0; /* 去掉默认的margin */  \r\n  overflow: hidden; /* 隐藏body的滚动条 */  \r\n}  \r\n\r\n.el-container {  \r\n  display: flex;  \r\n  flex-direction: column;  \r\n  height: 100%; /* 设置容器高度为100% */  \r\n}  \r\n\r\n.el-header {  \r\n  position: fixed;  \r\n  top: 0;  \r\n  left: 0;  \r\n  right: 0;  \r\n  background-color: var(--el-color-primary-light-3);  \r\n  color: black;  \r\n  height: 12%;   \r\n  z-index: 1000;  \r\n  display: flex;  \r\n  align-items: center;  \r\n  justify-content: space-between;  \r\n  padding: 0 20px;  \r\n}  \r\n.specific_grade {  \r\n  height: 100%;  \r\n  overflow-y: auto; /* 允许垂直滚动 */  \r\n} \r\n.badge {  \r\n  width: 85px;  \r\n  height: 85px;  \r\n  margin-left: 0;  \r\n  margin-right: 10px;  \r\n  border-radius: 50%;  \r\n}  \r\n\r\n.user-info {  \r\n  margin-left: 20px;   \r\n  margin-right: 50px;   \r\n}  \r\n.lab{\r\n    display: flex;\r\n    gap: 350px;\r\n    justify-content: center;  /* 垂直居中对齐 */\r\n    align-items: center;\r\n    margin-top: 50px;\r\n    font-size: 20px;\r\n}\r\n.el-table th {\r\n  font-weight: bold;  /* 设置表头字体加粗 */\r\n}\r\n</style>"],"mappings":"AA0FA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE,CAAC;MACTC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,CAAC;MACfC,aAAa,EAAE,KAAK;MAAE;MACtBC,aAAa,EAAE,EAAE;MAAE;MACnBC,WAAW,EAAE,IAAI;MAAE;MACnBC,QAAQ,EAAE,CACR;QAAEC,MAAM,EAAE,MAAM;QAAEC,QAAQ,EAAE,IAAI;QAAEC,UAAU,EAAE,EAAE;QAAEC,UAAU,EAAE,EAAE;QAAEC,UAAU,EAAE,EAAE;QAAEC,UAAU,EAAE,EAAE;QAAEC,QAAQ,EAAE,EAAE;QAAEC,SAAS,EAAE;MAAE,CAAC,EAC9H;QAAEP,MAAM,EAAE,MAAM;QAAEC,QAAQ,EAAE,IAAI;QAAEC,UAAU,EAAE,EAAE;QAAEC,UAAU,EAAE,EAAE;QAAEC,UAAU,EAAE,EAAE;QAAEC,UAAU,EAAE,EAAE;QAAEC,QAAQ,EAAE,EAAE;QAAEC,SAA