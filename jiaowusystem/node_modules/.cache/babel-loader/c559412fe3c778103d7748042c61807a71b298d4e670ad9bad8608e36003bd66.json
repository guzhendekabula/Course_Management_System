                 <span>{{ user_account }}</span>  \r\n                </div>    \r\n                <div class=\"info-item\">  \r\n                  <span>状态:</span>  \r\n                  <span v-if=\"user_status === 1\">启用</span>\r\n                  <span v-else-if=\"user_status === 0\">停用</span>  \r\n                </div>  \r\n            </el-card>\r\n          </div>\r\n          <!--教务员个人信息-->\r\n          <div class=\"card-container\" v-if=\"user_role === '教务员'\">\r\n            <div class=\"avatar-container\">\r\n              <img :src=\"user_picture\" alt=\"头像\" class=\"avatar\">\r\n              <div class=\"avatar-label\">教务员头像</div>\r\n            </div>\r\n            <el-card style=\"max-width: 800px\" class=\"tch_personal_box-card\">\r\n              <template #header>\r\n                <div class=\"card-header\">\r\n                  <span>教务员个人信息</span>\r\n                </div>\r\n              </template>\r\n                <div class=\"info-item\">  \r\n                  <span>教师id:</span>  \r\n                  <span>{{ user_id }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>姓名:</span>  \r\n                  <span>{{ user_name }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>等级:</span>  \r\n                  <span v-if=\"user_rank === 1\">院级</span>\r\n                  <span v-else-if=\"user_rank === 2\">校级</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>电话:</span>  \r\n                  <span>{{ user_tel }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>账号:</span>  \r\n                  <span>{{ user_account }}</span>  \r\n                </div>    \r\n                <div class=\"info-item\">  \r\n                  <span>状态:</span>  \r\n                  <span v-if=\"user_status === 1\"