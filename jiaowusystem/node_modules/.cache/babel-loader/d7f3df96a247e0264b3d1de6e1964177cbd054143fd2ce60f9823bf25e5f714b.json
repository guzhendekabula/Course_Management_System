{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      user_id: '',\n      user_name: '古镇的卡布拉',\n      user_account: '',\n      user_sex: '男',\n      user_sch: '软件工程学院',\n      user_grade: 22,\n      user_tel: '15013332183',\n      user_password: 'Kbl',\n      user_status: 1,\n      user_picture: require('@/assets/logo.png'),\n      isChangePersonalInfoRoute: false\n    };\n  },\n  watch: {\n    '$route'(to) {\n      this.checkRoute(to); // 监听路由变化  \n    }\n  },\n  created() {\n    // 从查询参数中获取 id 和账号\n    this.user_account = this.$route.query.user_account;\n    this.fetchUserInfo();\n    this.checkRoute(this.$route); // 确认当前路由\n  },\n  methods: {\n    fetchUserInfo() {\n      const url = `http://localhost:8080/personalPage/stu/${this.user_account}`;\n      axios.get(url).then(response => {\n        const data = response.data;\n        this.user_id = data.stu_id; // 更新 user_id 为 tch_id\n        this.user_name = data.stu_name; // 设置 user_name 为 tch_name\n        this.user_sex = data.stu_sex;\n        this.user_sch = data.stu_sch;\n        this.user_tel = data.stu_tel;\n        this.user_password = data.stu_password;\n        this.user_status = data.stu_status;\n        this.user_picture = require(data.stu_picture || '@/assets/logo.png');\n      }).catch(error => {\n        console.error('获取用户信息失败:', error);\n      });\n    },\n    checkRoute(route) {\n      // 检查当前路由是否是需要单独显示的页面  \n      const changepersonalInfoRoutes = ['stu_change_personal_info'];\n      this.isChangePersonalInfoRoute = changepersonalInfoRoutes.includes(route.name);\n    },\n    goToTeacherHome() {\n      this.$router.go(-1);\n    },\n    goToChangeInfo() {\n      this.$router.push({\n        name: 'stu_change_personal_info',\n        query: {\n          user_id: this.user_id,\n          user_account: this.user_account\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user_id","user_name","user_account","user_sex","user_sch","user_grade","user_tel","user_password","user_status","user_picture","require","isChangePersonalInfoRoute","watch","$route","to","checkRoute","created","query","fetchUserInfo","methods","url","get","then","response","stu_id","stu_name","stu_sex","stu_sch","stu_tel","stu_password","stu_status","stu_picture","catch","error","console","route","changepersonalInfoRoutes","includes","name","goToTeacherHome","$router","go","goToChangeInfo","push"],"sources":["D:\\vue_project\\test_project\\jiaowusystem2\\src\\views\\main\\stu_personal_info_interface.vue"],"sourcesContent":["<!--个人信息（老师，学生，教务）-->\r\n<template>\r\n  <div class=\"stu_personal_info\">\r\n    <div v-if=\"isChangePersonalInfoRoute\">  \r\n      <router-view></router-view> <!-- 仅渲染子组件 -->  \r\n    </div>  \r\n    <div v-if=\"!isChangePersonalInfoRoute\">\r\n      <el-container>  \r\n        <el-header> \r\n          <img src=\"@/assets/2.jpg\" alt=\"Icon\" class=\"badge\"> \r\n          <h3 style=\"margin-right: auto;\">某某学校学生端——个人信息页面</h3>    \r\n          <div class=\"user-info\">  \r\n            <h3>账号：{{ user_account }}</h3>  \r\n            <h3>姓名: {{ user_name }}</h3>  \r\n          </div>  \r\n          <el-button type=\"danger\" style=\"float: right;\" @click=\"goToTeacherHome\">返回</el-button>  \r\n        </el-header>  \r\n        <el-main>  \r\n          <div class=\"card-container\">\r\n            <div class=\"avatar-container\">\r\n              <img :src=\"user_picture\" alt=\"头像\" class=\"avatar\">\r\n              <div class=\"avatar-label\">学生头像</div>\r\n            </div>\r\n            <el-card style=\"max-width: 800px\" class=\"tch_personal_box-card\">\r\n              <template #header>\r\n                <div class=\"card-header\">\r\n                  <span>个人信息</span>\r\n                </div>\r\n              </template>\r\n                <div class=\"info-item\">  \r\n                  <span>学生id:</span>  \r\n                  <span>{{ user_id }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>姓名:</span>  \r\n                  <span>{{ user_name }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>性别:</span>  \r\n                  <span>{{ user_sex }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>学院:</span>  \r\n                  <span>{{ user_sch }}</span>  \r\n                </div> \r\n                <div class=\"info-item\">  \r\n                  <span>年级:</span>  \r\n                  <span>{{ user_grade }}级</span>  \r\n                </div> \r\n                <div class=\"info-item\">  \r\n                  <span>电话:</span>  \r\n                  <span>{{ user_tel }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>账号:</span>  \r\n                  <span>{{ user_account }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>密码:</span>  \r\n                  <span>{{ user_password }}</span>  \r\n                </div>  \r\n                <div class=\"info-item\">  \r\n                  <span>状态:</span>  \r\n                  <span v-if=\"user_status === 1\">启用</span>\r\n                  <span v-else-if=\"user_status === 0\">停用</span>  \r\n                </div>  \r\n              <template #footer>\r\n                <el-button type=\"primary\" @click=\"goToChangeInfo\">修改个人信息</el-button>\r\n              </template>\r\n            </el-card>\r\n          </div>\r\n        </el-main>  \r\n        <router-view></router-view>\r\n      </el-container>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'; \r\nexport default {\r\n  data() {\r\n    return {\r\n      user_id: '',\r\n      user_name: '古镇的卡布拉',\r\n      user_account: '',\r\n      user_sex: '男',\r\n      user_sch: '软件工程学院',\r\n      user_grade: 22,\r\n      user_tel: '15013332183',\r\n      user_password: 'Kbl',\r\n      user_status: 1,\r\n      user_picture: require('@/assets/logo.png'),\r\n      isChangePersonalInfoRoute: false\r\n    };\r\n  },\r\n  watch: {  \r\n    '$route'(to) {  \r\n      this.checkRoute(to); // 监听路由变化  \r\n    }  \r\n  }, \r\n  created() {\r\n    // 从查询参数中获取 id 和账号\r\n    this.user_account = this.$route.query.user_account;\r\n    this.fetchUserInfo();\r\n    this.checkRoute(this.$route); // 确认当前路由\r\n  },\r\n  methods: {\r\n    fetchUserInfo() {\r\n      const url = `http://localhost:8080/personalPage/stu/${this.user_account}`;\r\n      axios.get(url)\r\n        .then(response => {\r\n          const data = response.data;\r\n          this.user_id = data.stu_id; // 更新 user_id 为 tch_id\r\n          this.user_name = data.stu_name; // 设置 user_name 为 tch_name\r\n          this.user_sex = data.stu_sex;\r\n          this.user_sch = data.stu_sch;\r\n          this.user_tel = data.stu_tel;\r\n          this.user_password = data.stu_password;\r\n          this.user_status = data.stu_status;\r\n          this.user_picture = require(data.stu_picture || '@/assets/logo.png');\r\n        })\r\n        .catch(error => {\r\n          console.error('获取用户信息失败:', error);\r\n        });\r\n    },\r\n    checkRoute(route) {  \r\n      // 检查当前路由是否是需要单独显示的页面  \r\n      const changepersonalInfoRoutes = [  \r\n        'stu_change_personal_info' \r\n      ];  \r\n      this.isChangePersonalInfoRoute = changepersonalInfoRoutes.includes(route.name);  \r\n    },  \r\n    goToTeacherHome() {\r\n      this.$router.go(-1);\r\n    },\r\n    goToChangeInfo() {\r\n      this.$router.push({\r\n        name: 'stu_change_personal_info',\r\n        query: {\r\n          user_id: this.user_id,\r\n          user_account: this.user_account\r\n        }\r\n      });\r\n    }\r\n}\r\n}\r\n\r\n</script>\r\n\r\n<style lang='stylus' scoped>\r\nhtml, body {  \r\n  height: 100%; /* 设置html和body的高度为100% */  \r\n  margin: 0; /* 去掉默认的margin */  \r\n  overflow: hidden; /* 隐藏body的滚动条 */  \r\n}  \r\n\r\n.stu_personal_info {  \r\n  height: 100%; /* 设置容器高度为100% */  \r\n  overflow-y: auto; /* 允许垂直滚动 */  \r\n}  \r\n\r\n.el-container {  \r\n  display: flex;  \r\n  flex-direction: column;  \r\n  height: 100%; /* 设置容器高度为100% */  \r\n}  \r\n.el-header {  \r\n  position: fixed;  \r\n  top: 0;  \r\n  left: 0;  \r\n  right: 0;  \r\n  background-color: var(--el-color-primary-light-3);  \r\n  color: black;  \r\n  height: 12%;   \r\n  z-index: 1000;  \r\n  display: flex;  \r\n  align-items: center;  \r\n  justify-content: space-between;  \r\n  padding: 0 20px;  \r\n}  \r\n\r\n.el-main {  \r\n  flex-grow: 1;  \r\n  margin-top: 2.5%; /* 使内容下移，避开固定的header */  \r\n  background-color: white;  \r\n  color: black;  \r\n  overflow-y: auto; /* 允许主区域垂直滚动 */  \r\n}  \r\n\r\n.card-header {  \r\n  font-size: 28px;  \r\n  font-weight: bold;  \r\n}  \r\n\r\n.user-info {  \r\n  margin-left: 20px;   \r\n  margin-right: 50px;   \r\n}  \r\n\r\n.tch_personal_box-card {  \r\n  margin-left: 100px;  \r\n  width: 700px;  \r\n}  \r\n\r\n.info-item {  \r\n  display: flex;  \r\n  padding: 10px 0; /* 每行上下留出一些间距 */  \r\n  font-size: 28px;  \r\n}  \r\n\r\n.info-item span:first-child {  \r\n  font-weight: bold; /* 标签字体加粗 */  \r\n  min-width: 150px; /* 设置最小宽度以保证对齐 */   \r\n  margin-left: 200px;   \r\n}  \r\n\r\n.badge {  \r\n  width: 85px;  \r\n  height: 85px;  \r\n  margin-left: 0;  \r\n  margin-right: 10px;  \r\n  border-radius: 50%;  \r\n}  \r\n\r\n.card-container {  \r\n  display: flex;  \r\n}  \r\n\r\n.avatar-container {  \r\n  text-align: center; /* 使标签文本居中对齐 */  \r\n  margin-left: 200px;  \r\n  margin-top: 100px;  \r\n}  \r\n\r\n.avatar-label {  \r\n  margin-top: 10px; /* 标签与头像之间的间距 */  \r\n  font-size: 25px; /* 标签字体大小 */  \r\n  font-weight: bold;  \r\n}  \r\n\r\n.avatar {  \r\n  width: 250px; /* 设置头像宽度 */  \r\n  height: 250px; /* 设置头像高度 */  \r\n  border-radius: 50%; /* 使头像为圆形 */  \r\n}